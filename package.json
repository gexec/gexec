    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.474.0",
    "pinia": "^2.3.1",
    "radix-vue": "^1.9.13",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
